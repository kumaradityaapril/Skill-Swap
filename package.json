{
  "name": "skillswap",
  "version": "1.0.0",
  "description": "A Peer-to-Peer Learning Hub",
  "main": "index.js",
  "scripts": {
    "build": "echo \"Skipping root build - use 'npm run build:frontend' or 'npm run build:backend'\" && exit 0",
    "build:frontend": "cd frontend && npm install && npm run build",
    "build:production": "npm run build:frontend",
    "start": "cd frontend && npm run preview",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend-separate": "cd backend && npm run dev",
    "dev:frontend-separate": "cd frontend && npm run dev",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "vercel-build": "npm run build:frontend"
  },
  "keywords": [
    "skill-sharing",
    "learning",
    "mentoring",
    "peer-to-peer"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.10"
  },
  "dependencies": {
    "axios": "^1.9.0"
  }
}
